{
    "authors": [
        {
            "email": "fkooman@tuxed.net",
            "name": "Fran\u00e7ois Kooman"
        }
    ],
    "autoload": {
        "psr-4": {
            "fkooman\\SAML\\SP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "fkooman\\SAML\\SP\\Tests\\": "tests/"
        }
    },
    "description": "SAML Service Provider library",
    "license": "MIT",
    "name": "fkooman/php-saml-sp",
    "require": {
        "ext-date": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-session": "*",
        "ext-spl": "*",
        "ext-zlib": "*",
        "paragonie/constant_time_encoding": "^1|^2",
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4|^5|^6|^7"
    },
    "suggest": {
        "ext-libsodium": "PHP < 7.2 sodium implementation",
        "ext-sodium": "PHP >= 7.2 sodium implementation"
    },
    "support": {
        "email": "fkooman@tuxed.net",
        "source": "https://git.tuxed.net/fkooman/php-saml-sp"
    },
    "type": "library"
}
